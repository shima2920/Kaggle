        "#train\r\n",
        "total = train.isnull().sum().sort_values(ascending=False)\r\n",
        "percent = ((train.isnull().sum()/train.isnull().count())*100).sort_values(ascending=False)\r\n",
        "missing_data = pd.concat([total, percent], axis=1, keys=['Total', 'Percent'])\r\n",
