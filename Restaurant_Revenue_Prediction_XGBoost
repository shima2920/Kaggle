      "cell_type": "code",
      "metadata": {
        "id": "1mQ9AAab1yUM"
      },
      "source": [
        "# creating dummy variables\r\n",
        "df.Year = df.Year.astype(str)\r\n",
        "df.Month = df.Month.astype(str)\r\n",
        "Year_dummy = pd.get_dummies(df[['Year','Month']],drop_first=True)\r\n",
        "df = pd.concat([df,Year_dummy],axis=1)\r\n",
        "df.drop(['Year','Month'],axis=1, inplace=True)"
      ],
      "execution_count": 260,
      "outputs": []
